@model Askrindo.Areas.RefData.Models.OrgStruct.KantorCabangViewModel

@{
    ViewBag.Title = "KantorCabangDelete";
    Layout = "~/Areas/RefData/Views/Shared/_OrgStruct.cshtml";
}

<h3 style="padding-bottom: 0; margin-bottom: 0">Hapus Bagian</h3>
@*@Html.Partial("_Info_Dept", Model.DibawahDirektorat.Dept)*@

<div style="padding-bottom: 12px" class="warning">
    Kantor Cabang berikut akan dihapus. Lanjutkan?
</div>

<table class="warning">
    <tr>
        <td>ID</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.KantorCabang.BranchId)</td>
    </tr>
    <tr>
        <td>Direktorat</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.KantorCabang.Dept.DeptName)</td>
    </tr>
    <tr>
        <td>Cabang</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.KantorCabang.BranchName)</td>
    </tr>
    <tr>
        <td>Kelas</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.KantorCabang.BranchClass.ClassName)</td>
    </tr>
    <tr>
        <td>Kode Kantor</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.KantorCabang.BranchCode)</td>
    </tr>
    <tr>
        <td>Supporting Unit?</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.KantorCabang.IsSupporting)</td>
    </tr>
</table>

@using (Html.BeginForm()) {
    <div style="padding-top: 12px">
        <input type="submit" value="Hapus" /> |
        @Html.ActionLink("Batal", "KantorCabang")
    </div>
}

