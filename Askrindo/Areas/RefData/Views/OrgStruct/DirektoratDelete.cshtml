@model Askrindo.Models.Dept

@{
    ViewBag.Title = "DirektoratDelete";
    Layout = "~/Areas/RefData/Views/Shared/_orgstruct.cshtml";
}

<h3>Hapus Direktorat</h3>

<div style="padding-bottom: 12px" class="warning">
    Direktorat berikut akan dihapus. Lanjutkan?
</div>

<table class="warning">
    <tr>
        <td>ID</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.DeptId)</td>
    </tr>
    <tr>
        <td>Direktorat</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.DeptName)</td>
    </tr>
</table>

@using (Html.BeginForm()) {
    <div style="padding-top: 12px">
        <input type="submit" value="Hapus" /> |
        @Html.ActionLink("Batal", "Direktorat")
    </div>
}