@model Askrindo.Models.Branch

@{
    ViewBag.Title = "BranchDelete";
    Layout = "~/Areas/RefData/Views/Shared/_orgstruct.cshtml";
}

<h3 style="padding-bottom: 0; margin-bottom: 0">Hapus Cabang</h3>
@Html.Partial("_Info_Dept", Model.Dept)

<div style="padding-bottom: 12px" class="warning">
    Cabang berikut akan dihapus. Lanjutkan?
</div>

<table class="warning">
    <tr>
        <td>ID</td>
        <td style="width: 10px">:</td>
        <td>@Html.DisplayFor(model => model.BranchId)</td>
    </tr>
    <tr>
        <td>Kode</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.BranchCode)</td>
    </tr>
    <tr>
        <td>Nama Cabang</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.BranchName)</td>
    </tr>
    <tr>
        <td>Kelas</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.BranchClass.ClassName)</td>
    </tr>
    <tr>
        <td>Supporting Unit?</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.IsSupporting)</td>
    </tr>
</table>

@using (Html.BeginForm()) {
    <div style="padding-top: 12px">
        <input type="submit" value="Hapus" /> |
        @Html.ActionLink("Batal", "BranchList", new { deptid = Model.DeptId }, null)
    </div>
}
