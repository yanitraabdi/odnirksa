@model Askrindo.Areas.RefData.Models.OrgStruct.BizUnitViewModel

@{
    ViewBag.Title = "BizUnitDelete";
    Layout = "~/Areas/RefData/Views/Shared/_orgstruct.cshtml";
}

<h3 style="padding-bottom: 0; margin-bottom: 0">Hapus KUP</h3>
@Html.Partial("_Info_Branch", Model.BizUnit.Branch)

<div style="padding-bottom: 12px" class="warning">
    KUP berikut akan dihapus. Lanjutkan?
</div>

<table class="warning">
    <tr>
        <td>ID</td>
        <td style="width: 10px">:</td>
        <td>@Html.DisplayFor(model => model.BizUnit.BizUnitId)</td>
    </tr>
    <tr>
        <td>Nama KUP</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.BizUnit.BizUnitName)</td>
    </tr>
    <tr>
        <td>Kode Kantor</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.BizUnit.BizUnitCode)</td>
    </tr>
    <tr>
        <td>Supporting Unit?</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.BizUnit.IsSupporting)</td>
    </tr>
</table>

@using (Html.BeginForm()) {
    <div style="padding-top: 12px">
        <input type="submit" value="Hapus" /> |
        @Html.ActionLink("Batal", "BizUnitList", new { branchId = @Model.BizUnit.BranchId }, null)
    </div>
}
