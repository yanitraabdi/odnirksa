@model Askrindo.Models.bisni

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/RefData/Views/Shared/_RiskBiz.cshtml";
}

@using (Html.BeginForm())
{
    <table class="edit">
        <tr>
            <td>ID</td>
            <td>@Html.TextBoxFor(m => m.bisnisid)</td>
        </tr>
        <tr>
            <td>Peristiwa Resiko</td>
            <td>@Html.TextAreaFor(m => m.bisnis)</td>
        </tr>
    </table>
    <div style="padding-top: 12px">
        <input type="Submit" value="Hapus"/>
        <input type="Button" value="Batal" onclick="location.href='@Url.Action("index","RiskBiz")';return false;"/>
    </div>
}