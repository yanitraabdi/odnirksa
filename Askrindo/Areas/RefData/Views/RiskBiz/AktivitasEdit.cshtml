@model Askrindo.Models.bisnis_aktifitas

@{
    ViewBag.Title = "Edit";
    Layout = "~/Areas/RefData/Views/Shared/_Aktivitas.cshtml";
}

@using (Html.BeginForm())
{
    <table class="edit">
        
        <tr>
            <td>ID</td>
            <td>
                @Html.TextBoxFor(m => m.aktifitasid)
                @Html.ValidationMessageFor(m => m.aktifitasid)
             </td>
        </tr>
        <tr>
            <td>Aktivitas Bisnis</td>
            <td>@Html.TextAreaFor(m => m.aktifitas)</td>
        </tr>
    </table>
    
    <div style="padding-top: 12px">
        <input type="Submit" value="Simpan"/>
        <input type="Button" value="Batal" onclick="location.href='@Url.Action("AktivitasIndex", "RiskBiz", new { id = Model.bisnisid })';return false;"/>
    </div>
}