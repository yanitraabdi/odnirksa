@model Askrindo.Models.MitigationCat

@{
    ViewBag.Title = "MitigationCatDelete";
    Layout = "~/Areas/RefData/Views/Shared/_Mitigation.cshtml";
}

<h3>Hapus Kelompok Mitigasi</h3>

<div style="padding-bottom: 12px" class="warning">
    Kelompok Mitigasi berikut akan dihapus. Lanjutkan?
</div>

<table class="warning">
    <tr>
        <td>ID</td>
        <td>:</td>
        <td>
            @Html.DisplayFor(model => model.MitigationCatId)
        </td>
    </tr>
    <tr>
        <td>Nama Kelompok Mitigasi</td>
        <td>:</td>
        <td>
            @Html.DisplayFor(model => model.MitigationCatName)
        </td>
    </tr>
</table>

@using (Html.BeginForm()) {
    <div style="padding-top: 12px">
        <input type="submit" value="Hapus" />
    </div>
}

@Html.Partial("_Link_MitigationCatList")
