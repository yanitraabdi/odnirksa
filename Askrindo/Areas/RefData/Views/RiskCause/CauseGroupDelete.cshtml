@model Askrindo.Models.CauseGroup

@{
    ViewBag.Title = "CauseGroupDelete";
    Layout = "~/Areas/RefData/Views/Shared/_riskcause.cshtml";
}

<h3>Hapus Kelompok Sebab</h3>

<div style="padding-bottom: 12px" class="warning">
    Kelompok Sebab berikut akan dihapus. Lanjutkan?
</div>

<table class="warning">
    <tr>
        <td>ID</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.CauseGroupId)</td>
    </tr>
    <tr>
        <td>Nama Kelompok Sebab</td>
        <td>:</td>
        <td>@Html.DisplayFor(model => model.CauseGroupName)</td>
    </tr>
</table>

@using (Html.BeginForm()) {
    <div style="padding-top: 12px">
        <input type="submit" value="Hapus" /> |
        @Html.ActionLink("Batal", "CauseGroupList")
    </div>
}