@model Askrindo.Models.UserInfo

@{
    ViewBag.Title = "UnlockUser";
    Layout = "~/Areas/Admin/Views/Shared/_UserMaintenance.cshtml";
}

<h3>Unlock User</h3>

<div style="padding-bottom: 12px">
    User berikut akan di-unlock
</div>

<table class="display">
    <tr>
        <td>Name User</td>
        <td>:</td>
        <td>@Model.aspnet_User.UserName</td>
    </tr>
    <tr>
        <td>Name Lengkap</td>
        <td>:</td>
        <td>@Model.FullName</td>
    </tr>
</table>

@using (Html.BeginForm())
{ 
    <div style="padding-top: 12px">
        <input type="submit" value="Unlock User" /> |
        @Html.ActionLink("Batal", "Index")
    </div>
}